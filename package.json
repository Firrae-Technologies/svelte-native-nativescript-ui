  "license": "MIT",
  "bugs": {
    "url": "https://github.com/halfnelson/svelte-native-nativescript-ui/issues"
  },
  "homepage": "https://github.com/halfnelson/svelte-native-nativescript-ui#readme",
  "peerDependencies": {
    "svelte-native": "~0.8.0",
    "tns-core-modules": "~6.4.2"
  },
  "scripts": {
