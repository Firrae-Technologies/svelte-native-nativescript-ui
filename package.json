    "nativescript-ui-gauge": "~6.0.0",
    "nativescript-ui-autocomplete": "~6.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.20.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.0",
    "svelte": "^3.19.2",
